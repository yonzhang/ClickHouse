    "cmake.toolset": null,
    "cmake.buildArgs": [],
    "cmake.buildEnvironment": {},
    "cmake.buildToolArgs": [
        "-j10"
    ],
    //see https://github.com/llvm-mirror/clang-tools-extra/blob/master/clangd/tool/ClangdMain.cpp
    "clangd.arguments": [
        "-all-scopes-completion",
        "-background-index",
        "-compile-commands-dir=cmake-build-debug",
        "-completion-style=detailed",
        "-clang-tidy=false",
        "-debug-origin",
        "-function-arg-placeholders",
        "-header-insertion=iwyu",
        "-header-insertion-decorators",
        "-input-mirror-file=.vscode/clangd-lsp.log",
        "-log=info",
        "-pch-storage=disk",
        "-pretty",
        "-suggest-missing-includes",
    ],
    "clangd.trace": "",
    "cmake.configureOnOpen": true,
    "cmake.sourceDirectory": "${workspaceRoot}",
    "todo.embedded.include": [
        "src/**/*"
    ],
    "jshint.enable": false,
    "git.ignoreLimitWarning": true,
